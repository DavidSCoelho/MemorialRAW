<h1>Editar dados do cliente</h1>
    <hr>
    <form action="/detalheIndividualCliente/updateprod" method="POST">
        <div class="form-control">
            <input type="hidden" name="id" id="id" value="{{prod.id}}">
            <label for="nomecliente">Nome:</label>
            <input type="text" name="nomecliente" id="nomecliente" placeholder="Digite o nome do produto" value="{{prod.nome}}">
        </div>

            <div class="form-group">
            <label for="cpfcliente">CPF:</label>
            <input class="form-control" type="text" name="cpfcliente" placeholder="Digite seu cpf" value="{{prod.cpf}}">
        </div>

    <div class="form-group">
            <label for="idadecliente">Idade:</label>
            <input class="form-control" type="number" name="idadecliente" placeholder="Digite sua idade" value="{{prod.idade}}">
        </div>

            <div class="form-group">
            <label for="telefonecliente">telefone:</label>
            <input class="form-control" type="text" name="telefonecliente" placeholder="Digite seu telefone" value="{{prod.telefone}}">
        </div>
        <input type="submit" value="Editar"   >
    </form>

